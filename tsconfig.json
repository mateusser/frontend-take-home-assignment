{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "checkJs": false,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "paths": {
      "~*": ["./src/*"],
      "@icons/*": ["./src/assets/icons/*"],
      "@icons": ["./src/assets/icons"]
    },
    "sourceMap": true,
    "target": "es6",
  },
  "include": ["./src/**/*"],
}
